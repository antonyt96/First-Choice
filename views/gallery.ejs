<% include header %>


<div class="container">
	<div class="row">
		<h2 class="ml-auto mr-auto">Gallery</h2>
	</div>
	<div class="row" style="display:flex; flex-wrap:wrap;">
		<% images.forEach(function(image){%>
			<div class="col-lg-4">
				<div  >
					<img class="img-thumbnail" src="<%=image.image%>">
				</div>
			</div>

	<%	}); %>
	
	</div>

</div>


<% include footer %>